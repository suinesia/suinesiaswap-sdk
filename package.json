{
  "name": "@vividnetwork/swap-sdk",
  "version": "0.6.0",
  "description": "SDK for Aptoswap, Suiswap and other swap platforms",
  "author": "VividNetwork <vivid.network@outlook.com>",
  "repository": "https://github.com/vividnetwork/swap-sdk",
  "keywords": [
    "aptoswap",
    "suiswap",
    "aptos",
    "sui"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist build package .aptos",
    "docs": "typedoc --entryPoints src/index.ts",
    "build-without-clean": "tsc -p tsconfig.json",
    "build": "npm run clean && npm run build-without-clean",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\", \"types\": [\"node\", \"mocha\"], \"lib\": [\"ES2020\"] }' mocha -r ts-node/register 'src/**/*.test.ts'"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.8.5",
    "chai": "^4.3.7",
    "mocha": "^10.1.0",
    "prompt-sync": "^4.2.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "yaml": "^2.1.3"
  },
  "dependencies": {
    "@manahippo/move-to-ts": "^0.4.2",
    "@mysten/sui.js": "^0.31.0",
    "@suins/toolkit": "^1.1.1",
    "aptos": "^1.3.16",
    "axios": "^1.1.2"
  }
}
